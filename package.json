{
  "name": "project-sunfish",
  "private": true,
  "version": "0.1.0",
  "devDependencies": {
    "@types/react": "16.1.0",
    "@types/react-dom": "16.0.4",
    "mobx": "4.1.0",
    "mobx-react": "5.0.0",
    "npm-build-tools": "2.2.5",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "ts-loader": "4.1.0",
    "tslib": "1.9.0",
    "typescript": "2.8.1",
    "webpack": "4.3.0",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1",
    "webpack-merge": "4.1.2"
  },
  "scripts": {
    "bundle": "npm run -s bundle:android && npm run -s bundle:ios",
    "bundle:android": "n-copy -d \"app/App.Platform.Android/Assets\" -s \"public\" \"app.*\"",
    "bundle:ios": "n-copy -d \"app/App.Platform.iOS/Resources\" -s \"public\" \"app.*\"",
    "prepack": "webpack-cli --config webpack.prod.js && npm run -s bundle",
    "start": "npm install --silent && webpack-dev-server --config webpack.dev.js"
  }
}
